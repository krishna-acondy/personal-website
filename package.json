{
  "name": "krishna-acondy-personal-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf ../krishna-acondy.github.io/* && rm -rf dist",
    "build-for-deploy": "ng build --prod && cp -r ./dist/* ../krishna-acondy.github.io && cp ./CNAME ../krishna-acondy.github.io && cd ../krishna-acondy.github.io && cp index.html 404.html",
    "deploy": "npm run clean && npm run build-for-deploy && cd ../krishna-acondy.github.io && git add . && git commit -m \"Updated Website\" && git push"
  },
  "private": true,
  "dependencies": {
