    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.2.0-rc.0",
    "@angular/http": "^7.2.0-rc.0",
    "@angular/platform-browser": "^7.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^7.2.0-rc.0",
    "@angular/router": "^7.2.0-rc.0",
    "core-js": "^2.6.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "6.3.3",
